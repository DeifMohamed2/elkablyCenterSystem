                            <label class="form-label">المبلغ المتبقي:</label>
                            <input type="number" class="form-control" id="edit_remainingAmount_<%= course %>_<%= teacher.id %>" placeholder="المبلغ المتبقي" readonly>
                          </div>
                          <div class="col-md-6">
