                              <button type="button" class="btn btn-success" onclick="addInstallment('<%= teacher.id %>', '<%= course %>')">
                                <i class="material-symbols-rounded">add</i>
                              </button>
                            </div>
                          </div>
                        </div>
                        <div class="mt-3">
                          <h6>سجل الأقساط:</h6>
                          <div id="edit_installmentHistory_<%= course %>_<%= teacher.id %>" class="table-responsive">
                            <!-- Installment history will be populated here -->
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                 </div>
                 <% }) %>
               </div>
             </div>
             <% }) %>
           </div>


         
              <input type="hidden" id="editStudentId">
            </form>
          </div>
          <div class="modal-footer">
